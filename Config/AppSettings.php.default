<?php

namespace Config;

class AppSettings
{
    /** @var DatabaseConfig */
    private $databaseSettings;

    public function getDatabaseConfig(): DatabaseConfig
    {
        if (is_null($databaseSettings)) {
            return new DatabaseConfig(
                'HOST',
                'USERNAME',
                'PASSWORD',
                'NAME'
            );
        }

        return $databaseSettings;
    }
}
